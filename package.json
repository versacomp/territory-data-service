  "homepage": "https://github.com/rtercias/territory-data-service#readme",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "babel-runtime": "^6.26.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.1",
    "mysql": "^2.15.0",
    "promise-mysql": "^3.2.1",
    "util": "^0.11.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
